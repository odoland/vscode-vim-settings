{
  "workbench.colorTheme": "Monokai",
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.handleKeys": {
    "<cmd+d>": false
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": false,
    "editor.detectIndentation": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": false,
    "editor.detectIndentation": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": true
  },
  "[go]": {
    "editor.tabSize": 8,
    "editor.insertSpaces": false,
    "editor.detectIndentation": true,
  }
}